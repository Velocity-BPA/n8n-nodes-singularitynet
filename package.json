{
  "name": "n8n-nodes-singularitynet",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for SingularityNET providing 16 resources and 100+ operations for decentralized AI marketplace integration. Includes payment channels, staking, IPFS, and cross-chain bridge support.",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "singularitynet",
    "snet",
    "ai",
    "artificial-intelligence",
    "machine-learning",
    "blockchain",
    "ethereum",
    "cardano",
    "agix",
    "asi",
    "decentralized-ai",
    "ai-marketplace"
  ],
  "license": "BUSL-1.1",
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-singularitynet",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-singularitynet.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:bundle && gulp build:icons",
    "build:bundle": "node esbuild.config.js",
    "dev": "tsc --watch",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SingularitynetNetwork.credentials.js",
      "dist/credentials/SingularitynetPlatform.credentials.js",
      "dist/credentials/AiService.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Singularitynet/Singularitynet.node.js",
      "dist/nodes/Singularitynet/SingularitynetTrigger.node.js"
    ]
  },
  "dependencies": {
    "axios": "^1.6.2",
    "ethers": "^6.9.0",
    "protobufjs": "^7.2.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.15.0",
    "esbuild": "^0.19.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "glob": "^10.3.0",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
